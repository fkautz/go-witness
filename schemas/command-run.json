{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://witness.dev/attestations/command-run/v0.1",
  "properties": {
    "cmd": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "stdout": {
      "type": "string"
    },
    "stderr": {
      "type": "string"
    },
    "exitcode": {
      "type": "integer"
    },
    "processes": {
      "items": {
        "properties": {
          "program": {
            "type": "string"
          },
          "processid": {
            "type": "integer"
          },
          "parentpid": {
            "type": "integer"
          },
          "programdigest": {
            "patternProperties": {
              ".*": {
                "type": "string"
              }
            },
            "type": "object"
          },
          "comm": {
            "type": "string"
          },
          "cmdline": {
            "type": "string"
          },
          "exedigest": {
            "patternProperties": {
              ".*": {
                "type": "string"
              }
            },
            "type": "object"
          },
          "openedfiles": {
            "patternProperties": {
              ".*": {
                "patternProperties": {
                  ".*": {
                    "type": "string"
                  }
                },
                "type": "object"
              }
            },
            "type": "object"
          },
          "environ": {
            "type": "string"
          },
          "specbypassisvuln": {
            "type": "boolean"
          }
        },
        "additionalProperties": false,
        "type": "object",
        "required": [
          "processid",
          "parentpid"
        ]
      },
      "type": "array"
    }
  },
  "additionalProperties": false,
  "type": "object",
  "required": [
    "cmd",
    "exitcode"
  ],
  "title": "command-run attestation",
  "description": "command-run internal type attestation"
}
